{
  "name": "devextreme-vue",
  "version": "19.2.8",
  "description": "DevExtreme Vue UI and Visualization Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevExpress/devextreme-vue.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run clean && npm run generate",
    "clean": "gulp clean",
    "generate": "gulp generate",
    "lint": "gulp lint",
    "pack": "lerna run --stream pack && gulp npm.pack",
    "serve": "webpack-dev-server",
    "test": "jest && lerna run --stream test",
    "test-watch": "jest --watch",
    "bootstrap-strategy": "lerna bootstrap",
    "update-integration-meta": "dx-tools update-integration-meta --output-path ./tools/integration-data.json"
  },
  "keywords": [
    "vue",
    "devextreme",
    "devexpress"
  ],
  "author": "Developer Express Inc.",
  "license": "MIT",
  "peerDependencies": {
    "devextreme": "~19.2.8",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^13.11.1",
    "@vue/test-utils": "^2.0.0-beta.3",
    "css-loader": "^2.1.1",
    "dasherize": "^2.0.0",
    "del": "^3.0.0",
    "devextreme": "~19.2.8",
    "devextreme-internal-tools": "stable",
    "dot": "^1.1.2",
    "gulp": "^4.0.0",
    "gulp-header": "^2.0.5",
    "gulp-shell": "^0.6.5",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0",
    "jest": "^24.8.0",
    "lerna": "^3.22.1",
    "minimatch": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.20.3",
    "ts-jest": "^24.1.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "^4.0.2",
    "url-loader": "^1.1.1",
    "vue": "^3.0.0-rc.12",
    "vue-loader": "^14.2.3",
    "vue-template-compiler": "^2.6.3",
    "webpack": "^4.29.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "mitt": "^2.1.0"
  }
}
